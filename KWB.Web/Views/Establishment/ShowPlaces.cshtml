                            <div style="right: -150px;position: relative; top: -45px;" class="form-group">
                                <input type="checkbox" id="isOpenValue" name="vehicle1">
                                <label for="isOpenValue"> Permanently_Closed</label>
                            </div>
                            <!-- Tags -->
                            <div style="margin-top: -40px;" class="form-group">
                                <label>Tags</label>
                                <select id="tagsValue" onchange="selectTag()" class="form-control" multiple="multiple" data-placeholder="Select the tags" style="width: 100%;">
