                                        <th>Tag Name</th>
                                        <th>Picture</th>
                                        <th>Selected</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var tag in Model.Markers)
                                    {
                                        <tr style="display: none;">
                                            <td style="display:none">@tag.TagID</td>
                                            <td >@tag.TagName</td>
                                            <td style="text-align: center;"><img style="width: 30px;" src="data:image/png;base64,@tag.Marker" alt=""/></td>
                                            <td><div style="text-align: center;"><input style="width: 30px;height: 16px;" type="checkbox" class="markers" id="marker @tag.TagID" onclick="selectMarker(@tag.TagID)" /></div></td>
