@model KWB.Web.Models.NewFolder.ResponsePlace
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- Google Font: Source Sans Pro -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700&display=fallback">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="../plugins/fontawesome-free/css/all.min.css">
    <!-- daterange picker -->
    <link rel="stylesheet" href="../../plugins/daterangepicker/daterangepicker.css">
    <!-- iCheck for checkboxes and radio inputs -->
    <link rel="stylesheet" href="../../plugins/icheck-bootstrap/icheck-bootstrap.min.css">
    <!-- Bootstrap Color Picker -->
    <link rel="stylesheet" href="../../plugins/bootstrap-colorpicker/css/bootstrap-colorpicker.min.css">
    <!-- Tempusdominus Bootstrap 4 -->
    <link rel="stylesheet" href="../../plugins/tempusdominus-bootstrap-4/css/tempusdominus-bootstrap-4.min.css">
    <!-- Select2 -->
    <link rel="stylesheet" href="../../plugins/select2/css/select2.min.css">
    <link rel="stylesheet" href="../../plugins/select2-bootstrap4-theme/select2-bootstrap4.min.css">
    <!-- Toastr -->
    <link rel="stylesheet" href="../../plugins/toastr/toastr.min.css">
    <!-- BS Stepper -->
    <link rel="stylesheet" href="../../plugins/bs-stepper/css/bs-stepper.min.css">
    <!-- dropzonejs -->
    <link rel="stylesheet" href="../../plugins/dropzone/min/dropzone.min.css">
    <!-- DataTables -->
    <link rel="stylesheet" href="../plugins/datatables-bs4/css/dataTables.bootstrap4.min.css">
    <link rel="stylesheet" href="../plugins/datatables-responsive/css/responsive.bootstrap4.min.css">
    <link rel="stylesheet" href="../plugins/datatables-buttons/css/buttons.bootstrap4.min.css">
    <!-- Theme style -->
    <link rel="stylesheet" href="../css/adminlte.min.css">
    <style>
        #map {
            position: absolute;
            height: 15%;
            width: 85%;
        }

        #searchInput {
            margin-top: 10px;
            height: 35px;
            width: 50px;
            font-size: 15px;
        }
        listColumn {
            height: 270px;
            overflow-y: scroll;
            float: left;
        }

        #tablePlace_length {
            right: -165%;
            top: -40px;
            position: relative;
        }
    </style>
</head>
<body class="hold-transition sidebar-mini" style=" background-color: gray;">
    <div style="width: 120%; word-break: break-all; " class="row">
        <div class="col-12">
            <div class="card">
                <div style="background-color: #007bff;" class="card-header">
                    <h3 class="card-title" style="color:white;">Establishment</h3>
                </div>
                <!-- /.card-header -->
                <div class="card-body">
                    <button type="button" onClick="addPlace()" class="btn btn-success"> Add Establishment</button>
                    <button type="button" onclick="searchWithGoogle()" class="btn btn-success"> Search with google</button>
                    <table id="tablePlace" class="table table-bordered table-striped">
                        <thead>
                            <tr>
                                <th>Name</th>
                                <th>Address</th>
                                <th>Phone</th>
                                <th>Website</th>
                                <th>Main Picture</th>
                                <th>Enable</th>
                                <th>Certification</th>
                                <th>Tags</th>
                                <th>Tags Qty</th>
                                <th>Last updated</th>
                                <th>Compare With Google</th>
                                <th>Edit</th>
                                <th>Delete</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var place in Model.PlacesDB)
                            {
                                <tr>
                                    <td>@place.Name</td>
                                    <td>@place.Address</td>
                                    <td>@place.Phone</td>
                                    <td style="width: 20%"><a href="@place.Website" alt="" target="_blank">@place.Website</a></td>
                                    <td>
                                        @if (place.Photo != null)
                                        {
                                            <img style="height: 150px; width: 150px;" src=@place.Photo alt="photo">
                                        }
                                    </td>
                                    <td>@place.Enable</td>
                                    <td>@place.CertificationName</td>
                                    <td style="word-break: initial;" id="listColumn">
                                        @if (place.Tags != null)
                                        {
                                            @foreach (var item in place.Tags)
                                            {
                                                <li style="list-style: auto;">
                                                    @item
                                                </li>
                                            }
                                        }
                                    </td>
                                    <td>@place.NumberOfTags</td>
                                    <td>@place.LastUpdated</td>
                                    <td style="text-align: center; ">
                                        @if (place.GooglePlaceID != null)
                                        {
                                            <button type="button" id="myBtnUpdate" value="@place.GooglePlaceID" onclick='updateEstablishmentData(@place.PlaceID)' class="btn btn-success " data-toggle="modal" data-target="#modal-updateByGoogle">
                                                <i class="nav-icon fa fa-upload"></i>
                                            </button>
                                            <div style="text-align: center; margin-top: 20px;">
                                                <input type="checkbox" style="width:20px;height:20px;" id="update @place.PlaceID" value="@place.Name @place.Address" onclick="addSelectPlace(@place.PlaceID)" />
                                            </div>
                                        }else{
                                            <button type="button" id="myBtnUpdate" value="@place.GooglePlaceID" onclick='updateEstablishmentData(@place.PlaceID)' class="btn btn-danger" data-toggle="modal" data-target="#modal-updateByGoogle">
                                                <i class="nav-icon fa fa-upload"></i>
                                            </button>
                                            <div style="text-align: center; margin-top: 20px;">
                                                <input type="checkbox" style="width:20px;height:20px;" value="@place.Name @place.Address" id="update @place.PlaceID" onclick="addSelectPlace(@place.PlaceID)" />
                                            </div>
                                        }
                                    </td>
                                    <td style="text-align: center; "><button type="button" onClick="updateEstablishment(@place.PlaceID)" class="btn btn-default" data-toggle="modal" data-target="#modal-update"><i class="nav-icon fas fa-edit"></i></button></td>
                                    <td style="text-align: center; "><button type="button" onClick="deletePlace(@place.PlaceID)" class="btn btn-danger" data-toggle="modal" data-target="#modal-delete"><i class="nav-icon fa fa-times"></i></button></td>
                                </tr>
                             }
                        </tbody>
                        <tfoot>
                            <tr>
                                <th>Name</th>
                                <th>Address</th>
                                <th>Phone</th>
                                <th>Website</th>
                                <th>Picture</th>
                                <th>Enable</th>
                                <th>Certification</th>
                                <th>Tags</th>
                                <th>Tags Qty</th>
                                <th>Last Updated</th>
                                <th>Compare With Google</th>
                                <th>Edit</th>
                                <th>Delete</th>
                            </tr>
                        </tfoot>
                    </table>
                </div>
            </div>
        </div>
    </div>

    <!--Confirm Compare With Google-->
    <div class="modal fade" id="modal-updateByGoogle">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <h4 class="modal-title">Establishment</h4>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <p>Do you want to compare with google this establishment?</p>
                    <!-- Establishment ID -->
                    <input type="hidden" id="estabishmentId">
                    <!-- Google ID -->
                    <input type="hidden" id="idGoogleValue">
                </div>
                <div class="modal-footer justify-content-between">
                    <button type="button" class="btn btn-default" data-dismiss="modal">No</button>
                    <button type="button" id="btnsubmitupdate" class="btn btn-success">Yes</button>
                </div>
            </div>
        </div>
    </div>

    <!--Update-->
