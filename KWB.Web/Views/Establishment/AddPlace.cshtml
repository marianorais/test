    <link rel="stylesheet" href="../css/adminlte.min.css">
    <style>
        #map {
            position: absolute;
            height: 9%;
            top: 75px;
            width: 45%;
        }

        #searchInput {
            margin-top: 10px;
            height: 35px;
            width: 50px;
            font-size: 15px;
        }

        #firstInput {
            position: relative;
            left: 53%;
            width: 47%;
        }
        .without_ampm::-webkit-datetime-edit-ampm-field {
           display: none;
         }

        input[type="time"]::-webkit-calendar-picker-indicator { background: none; display:none; }
    </style>
