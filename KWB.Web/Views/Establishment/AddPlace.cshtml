<body style=" background-color: gray;">
    <div class="content-wrapper">

        <div class="row">
            <div class="col-md-12">
                <div class="card card-primary">
                    <div class="card-header">
                        <h3 class="card-title" style="color:white;">Add a new Establishment</h3>
                    </div>
                    <form id="placeform">
                        <div class="card-body">
                            <!--Map-->
                            <div class="form-group">
                                <!-- Search input -->
                                <input id="searchInput" class="controls" type="text" style="width:330px;" placeholder="Looking For...">
                                <!-- Google map -->
                                <div id="map"></div>
                            </div>
                            <div id="firstInput">
                                <!-- Place Name -->
                                <div class="form-group">
                                    <label for="exampleInputEmail1">Establishment *</label>
                                    <input asp-for="Name" name="name" class="form-control" id="nameValue" placeholder="Enter name" maxlength="150">
                                </div>
                                <!-- Latitude -->
                                <div class="form-group">
                                    <label for="exampleInputPassword1">Latitude *</label>
                                    <input asp-for="Latitude" type="number" name="latitude" class="form-control" id="latValue" placeholder="Enter latitude">
                                </div>
                                <!-- Longitude -->
                                <div class="form-group">
                                    <label for="exampleInputPassword1">Longitude *</label>
                                    <input asp-for="Longitude" type="number" name="longitude" class="form-control" id="lngValue" placeholder="Enter longitude">
                                </div>
                                
                                <!-- PostalCode -->
                                <div class="form-group">
                                    <label for="exampleInputPassword1">ZipCode</label>
                                    <input asp-for="ZipCode" name="postalCode" class="form-control" id="postalCodeValue" placeholder="Enter postalCode" maxlength="50">
                                </div>
                                <!-- Location -->
                                <div class="form-group">
                                    <label for="exampleInputPassword1">Address *</label>
                                    <input asp-for="Location" name="location" class="form-control" id="locationValue" placeholder="Enter location" maxlength="300">
                                </div>
                            </div>
                            <!-- Permanently_Closed -->
                            <div class="form-group">
                                <input type="checkbox" id="isOpenValue">
                                <label> Permanently Closed</label>
                            </div>
                            <!-- Enable -->
                            <div class="form-group">
                                <input type="checkbox" checked id="enableValue" name="vehicle1">
                                <label for="isMainValue"> Enable</label>
                            </div>
                            <!--Not Main Tags -->
                            <div style="width: 50%;" class="form-group">
                                <label>Tags</label>
                                <select id="tagsValue" class="select2" multiple="multiple"  data-placeholder="Select the tags" style="width: 100%;">
                                    @foreach (var tag in Model.TagsNotMainDB.Where(a=>a != "Questionable" && a != "NOT KOSHER").ToList())
                                    {
                                        <option  value="@tag">@tag</option>
                                    }
                                </select>
                            </div>
                            <div style="display: flex;">
                                <!--Is must_see!-->
                                <div style="position: relative; left: 53%; width: 47%; top: -50px; margin-bottom: -2.5%; " class="form-group">
                                    <input type="checkbox" id="cantMissValue" name="vehicle1">
                                    <label for="isMainValue"> Must See</label>
                                </div>
                                <!--Is Questionable-->
                                <div style="position: relative; left: 33%; width: 47%; top: -50px; margin-bottom: -2.5%; " class="form-group">
                                    <input type="checkbox" id="questionableValue" name="vehicle1">
                                    <label for="isMainValue"> Questionable</label>
                                </div>
                                <!--Is Not Kosher-->
                                <div style="position: relative; left: 15%; width: 47%; top: -50px; margin-bottom: -2.5%; " class="form-group">
                                    <input type="checkbox" id="notKosherValue" name="vehicle1">
                                    <label for="isMainValue"> Not Kosher</label>
                                </div>
                            </div>
                            <!-- CertificationName -->
                            <div class="row">
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <label>Certification</label>
                                        <select id="certValue" class="form-control select2" style="width: 100%;">
                                            <option selected="selected">None</option>
                                            @foreach (var certificationName in Model.Certifications.Select(a => a.Name))
                                            {
                                                <option>@certificationName</option>
                                            }
                                        </select>
                                    </div>
                                </div>
                            </div>
                            <!-- Principal Tags Main -->
                            <div class="row">
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <label>Main Tag *</label>
                                        <select id="principalTagsValue" class="select2" style="width: 100%;">
                                            <option selected="selected">Select One</option>
                                            @foreach (var tag in Model.TagsMainDB)
                                            {
                                                <option onclick="principalTag()" value=@tag>@tag</option>
                                            }
                                        </select>
                                    </div>
                                </div>
