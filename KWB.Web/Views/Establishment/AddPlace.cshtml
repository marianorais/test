        function responseHour(day, hour, isOpenTime) {
            if (hour == null || hour.length < 1) { hour = "00:00" }
            else if(!hour.includes(':')){hour = correctHour(hour,2)}
            switch (day) {
                case 0:
                    isOpenTime ? document.getElementById('opSundayValue').value = hour : document.getElementById('clSundayValue').value = hour
                    break;
                case 1:
                    isOpenTime ? document.getElementById('opMondayValue').value = hour : document.getElementById('clMondayValue').value = hour
                    break;
                case 2:
                    isOpenTime ? document.getElementById('opTuesdayValue').value = hour : document.getElementById('clTuesdayValue').value = hour
                    break;
                case 3:
                    isOpenTime ? document.getElementById('opWednesdayValue').value = hour : document.getElementById('clWednesdayValue').value = hour
                    break;
                case 4:
                    isOpenTime ? document.getElementById('opThursdayValue').value = hour : document.getElementById('clThursdayValue').value = hour
                    break;
                case 5:
                    isOpenTime ? document.getElementById('opFridayValue').value = hour : document.getElementById('clFridayValue').value = hour
                    break;
                case 6:
                    isOpenTime ? document.getElementById('opSaturdayValue').value = hour : document.getElementById('clSaturdayValue').value = hour
                    break;
                default:
                    console.log("not found");
            }
        }
        function correctHour(time,position) {
           return time.slice(0, position) + ":" + (time).slice(position);  
        }
