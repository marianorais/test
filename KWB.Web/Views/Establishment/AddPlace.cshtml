                            <!-- Markers -->
                            <table id="markers" class="table table-bordered table-striped">
                                <button style="position: relative;top: 10px;" type="button" onclick="removeMarkersSelected()" class="btn btn-danger">
                                    Remove check markers
                                </button>
                                <thead>
                                    <tr>
                                        <th>Tag Name</th>
                                        <th>Picture</th>
                                        <th>Selected</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var tag in Model.Markers)//markers
                                    {
                                        <tr style="display: none;">
                                            <td >@tag.TagName</td>
                                            <td style="text-align: center;"><img style="width: 30px;" src="data:image/png;base64,@tag.Marker" alt=""/></td>
                                            <td><div style="text-align: center;"><input style="width: 30px;height: 16px;" type="checkbox" class="markers" id="marker @tag.TagID" onclick="selectMarker(@tag.TagID)" /></div></td>
                                        </tr>
                                     }
                                </tbody>
                                <tfoot>
                                    <tr>
                                        <th>Tag Name</th>
                                        <th>Picture</th>
                                        <th>Selected</th>
                                    </tr>
                                </tfoot>
                            </table>

